<Window x:Class="HostListeWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HostListeWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnImport" Content="Importieren" HorizontalAlignment="Left" Margin="615,177,0,0" VerticalAlignment="Top" Width="151" Height="33" Click="btnImport_Click"/>
        <Button x:Name="btnExport" Content="Exportieren" HorizontalAlignment="Left" Margin="615,229,0,0" VerticalAlignment="Top" Width="151" Height="33" Click="btnExport_Click"/>
        <Button x:Name="btnBeenden" Content="Beenden" HorizontalAlignment="Left" Margin="615,335,0,0" VerticalAlignment="Top" Width="151" Height="35" Click="btnBeenden_Click"/>
        <Button x:Name="btnClear" Content="Reset" HorizontalAlignment="Left" Margin="615,282,0,0" VerticalAlignment="Top" Width="151" Height="35" Click="btnClear_Click"/>
        <TextBox x:Name="txtAusgabe" HorizontalAlignment="Left" Height="346" Margin="10,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="556"/>

    </Grid>
</Window>
