<Window x:Class="Tic_Tac_Toe.WindowMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tic_Tac_Toe"
        mc:Ignorable="d"
        Title="WindowMenu" Height="300" Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image HorizontalAlignment="Left" Height="114" Margin="24,10,0,0" VerticalAlignment="Top" Width="349" Source="Logo.png"/>
        <Image HorizontalAlignment="Center" Height="68" Margin="0,146,0,0" VerticalAlignment="Top" Width="108" Source="TwoPlayer.png"/>
        <Button x:Name="btnTwoPlayer" Content="2 Player" HorizontalAlignment="Center" Margin="0,209,0,0" VerticalAlignment="Top" Width="136" Height="41" Click="btnTwoPlayer_Click"/>

    </Grid>
</Window>
